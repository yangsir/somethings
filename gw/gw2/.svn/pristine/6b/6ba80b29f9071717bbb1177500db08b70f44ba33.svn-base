body{
	margin:0px;
	text-align:center;
	font-size:12px;
	font-family:Arial, Helvetica, sans-serif;
	background:#fff; }
body img{
	border:none;}
#container{
	margin:0px auto;
	padding:0px;
	width:1002px;
	text-align:left;}
#flash{
	margin:0px; padding:0px;
	width:951px;
	height:336px;}
#top{
	margin:0px; padding:0px;
	width:1002px;
	height:336px;}
#top div{
	float:left;
	margin:0px; padding:0px;}
#nav{
	margin:0px; padding:0px;
	width:760px;
	height:39px;
	position: absolute;
	top:300px;
	left:350px}
#nav div{
	margin:0px; padding:0px;
	float:left;}
#nav ul{
	margin:0px; padding:3px 0px 7px 5px;}
#nav ul li{
	list-style:none;
	float:left;}
#nav ul li a{
	font-size:12px;
	text-decoration:none;
	color:#000;}
#nav ul li a:hover{
	font-size:12px;
	text-decoration:none;
	color: #F00;}
#main{
	margin:0px 0px 0px 25px; padding:0px;
	width:977px;
	height:560px;
	background:url(../images/xian_bg.jpg) repeat-x;
	clear:left;}
#fenlei{
	margin:0px 28px 0px 0px; padding:30px 0px 0px 0px;
	width:245px;
	float:left;}
#fenlei div{
	margin-bottom:12px;}
#main_right{
	float:left;
	width:704px;
	height: auto;
	margin:0px; padding:0px;}
#jj{
	margin:30px 0px 18px 0px; padding:0px;
	width:436px;
	height:auto;}
#zhanshi{
	margin:0px; padding:0px;
	float:left;
	width:436px;}
#zhanshi #img tr td img{
	margin:0px; padding:3px;
	border:1px solid #CCC;
	margin-bottom:10px;}
#link{
	margin:0px; padding:0px;
	float:left;
	width:258px;}
#footer{
	background:url(../images/footer_bg.jpg) repeat-x;
	width:952px;
	height:81px;
	color:#D1FB94;
	line-height:24px;
	clear:left;
	margin:0px 25px;}

#content{
	margin:0px 25px; padding:0px;
	width:952px;
	height:auto;
	background:url(../images/xian_bg.jpg) repeat-x;
	clear:left;}	
#right{
	float:left;
	width:679px;
	height: auto;
	margin:0px; padding:0px;}
#news_border tr td{
	border-bottom:1px dotted #999;}
#pic tr td img{
	padding:2px;
	border:1px solid #CCC;}
	
	
	
